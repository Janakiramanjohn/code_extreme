// output


1111112
3222222
3333334
5444444
5555556
7666666







public class Simple{
public static void main(String args[]){
int n=2;
for(int i=1;i<=n;i++)
{
if(i%2==0)
{
System.out.print(i+1);
for(int j=0;j<n;j++)
{
System.out.print(i);
}
System.out.println();
}
else 
{
for(int j=0;j<n;j++)
{
System.out.print(i);
}
System.out.print(i+1);
System.out.println();
}
}
}
}
